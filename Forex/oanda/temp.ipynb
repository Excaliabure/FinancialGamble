    "iters = 50\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x = np.zeros(500)\n",
    "x[0] = 2\n",
    "x[1] = 1\n",
    "for n in range(2,500):\n",
    "    x[n] = 2 * x[n-1] - x[n-2]/2\n",
    "\n",
    "y = np.zeros(500)\n",
    "for n in range(len(x)):\n",
    "    y[n] = x[n] / ((1 + np.sqrt(2)/2) ** n)\n",
    "\n",
    "# plt.plot(y)\n",
    "y[-1]"
   ]
